% Encoding: UTF-8

% requires siunitx.sty

@symbol{area,
  name={square metre},
  symbol={\si{\metre\squared}},
  description={area},
  identifier={derivedunit}
}

@symbol{volume,
  name={cubic metre},
  symbol={\si{\metre\cubed}},
  description={volume},
  identifier={derivedunit}
}

@symbol{velocity,
  name={metre per second},
  symbol={\si{\metre\per\second}},
  description={velocity},
  identifier={derivedunit}
}

@symbol{acceleration,
  name={metre per second squared},
  symbol={\si{\metre\per\square\second}},
  description={acceleration},
  identifier={derivedunit}
}

@symbol{density,
  name={ampere per square metre},
  symbol={\si{\ampere\per\square\metre}},
  description={density},
  identifier={derivedunit}
}

@symbol{luminance,
  name={candela per square metre},
  symbol={\si{\candela\per\square\metre}},
  description={luminance},
  identifier={derivedunit}
}

@symbol{specificvolume,
  name={cubic metre per kilogram},
  symbol={\si{\cubic\metre\per\kilogram}},
  description={specific volume},
  identifier={derivedunit}
}

@symbol{concentration,
  name={mole per cubic metre},
  symbol={\si{\mole\per\cubic\metre}},
  description={concentration},
  identifier={derivedunit}
}

@symbol{wavenumber,
  name={per metre},
  symbol={\si{\per\metre}},
  description={wave number},
  identifier={derivedunit}
}
