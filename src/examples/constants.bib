% Encoding: UTF-8

@constant{pi,
  constantname={pi},
  constantsymbol={\ensuremath{\pi}},
  definition={the ratio of the length of the circumference
    of a circle to its diameter},
  value={3.14159},
  identifier={constant}
}

@constant{eulercons,
  constantname={Euler's constant},
  constantsymbol={\ensuremath{\gamma}},
  definition={the limit of \[\sum_{r=1}^n\frac{1}{r}-\ln n\]
    as $n\to\infty$},
  value={0.57721},
  identifier={constant}
}

@constant{e,
  constantname={Euler's number},
  constantsymbol={\ensuremath{e}},
  definition={base of natural logarithms},
  value={2.71828},
  identifier={constant}
}

@constant{root2,
  constantname={Pythagoras' constant},
  constantsymbol={\ensuremath{\surd2}},
  definition={the square root of 2},
  value={1.41421},
  identifier={constant}
}

@constant{goldenratio,
  constantname={golden ratio},
  constantsymbol={\ensuremath{\phi}},
  definition={the ratio $\frac{1+\surd5}{2}$},
  value={1.61803},
  identifier={constant}
}

@constant{aperysconstant,
  constantname={Ap\'ery's constant},
  constantsymbol={\ensuremath{\zeta(3)}},
  definition={a special value of the Riemann zeta function},
  value={1.2020569},
  identifier={constant}
}

@constant{conwaysconstant,
  constantname={Conway's constant},
  constantsymbol={\ensuremath{\lambda}},
  definition={the invariant growth rate of all derived strings},
  value={1.30357},
  identifier={constant}
}

@constant{zero,
  constantname={zero},
  constantsymbol={0},
  definition={nothing or nil},
  identifier={constant}
}

@constant{one,
  constantname={one},
  constantsymbol={1},
  definition={single entity, unity},
  identifier={constant}
}

@constant{imaginary,
  constantname={imaginary unit},
  constantsymbol={\ensuremath{i}},
  definition={defined as $i^2 = -1$},
  identifier={constant},
  alternative={\ensuremath{j}}
}
