% Encoding: UTF-8

@dualsymbol{pi,
   name={pi},
   symbol={\ensuremath{\pi}},
   description={the ratio of the length of the circumference
    of a circle to its diameter},
   user1={3.14159}
}

@dualsymbol{eulercons,
  name={Euler's constant},
  symbol={\ensuremath{\gamma}},
  description={the limit of \[\sum_{r=1}^n\frac{1}{r}-\ln n\]
    as $n\to\infty$},
  user1={0.57721}
}

@dualsymbol{e,
  name={Euler's number},
  symbol={\ensuremath{e}},
  description={base of natural logarithms},
  user1={2.71828}
}

@dualsymbol{root2,
  name={Pythagoras' constant},
  symbol={\ensuremath{\surd2}},
  description={the square root of 2},
  user1={1.41421}
}

@dualsymbol{goldenratio,
  name={golden ratio},
  symbol={\ensuremath{\phi}},
  description={the ratio $\frac{1+\surd5}{2}$},
  user1={1.61803}
}
