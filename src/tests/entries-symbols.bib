% Encoding: UTF-8

@preamble{
"\providecommand{\mtx}[1]{\boldsymbol{#1}}
\providecommand{\set}[1]{\mathcal{#1}}
\providecommand{\card}[1]{|\set{#1}|}
\providecommand{\imaginary}{i}"}

@symbol{M,
  name={$\mtx{M}$},
  text={\mtx{M}},
  user1={mtx},
  description={a \gls{ext1.matrix}}
}

@symbol{v,
  name={$\vec{v}$},
  text={\vec{v}},
  user1={vec},
  description={a \gls{ext1.vector}}
}

@symbol{S,
  name={$\set{S}$},
  text={\set{S}},
  user1={set},
  description={a \gls{ext1.set}}
}

@symbol{U,
  name={$\set{U}$},
  text={\set{U}},
  description={the universal \gls{ext1.set}}
}

@symbol{emptyset,
  name={$\emptyset$},
  text={\emptyset},
  description={the set containing no elements}
}

@symbol{card,
  name={$\card{S}$},
  text={\card{S}},
  user1={card},
  description={the \gls{ext1.cardinality} of the \gls{ext1.set}
  $\set{S}$}
}

@symbol{fact,
  name={$!$},
  text={!},
  description={factorial}
}

@symbol{i,
  name={$\imaginary$},
  text={\imaginary},
  description={square root of minus one ($\sqrt{-1}$)}
}

@symbol{angstrom,
  name={\AA},
  description={\AA ngstr\"om}
}
